{
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "semi": "error",
    "quotes": [
      "error",
      "single"
    ],
    "max-len": [
      "error",
      120
    ],
    "no-unused-vars": "warn",
    "import/prefer-default-export": "off"
  }
}
