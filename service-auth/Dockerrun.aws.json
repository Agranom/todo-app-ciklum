{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "service-auth",
      "image": "agranom/service-auth:VERSION",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3001
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "PORT",
          "value": 3001
        }
      ]
    }
  ]
}
