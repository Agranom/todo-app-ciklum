{
  "AWSEBDockerrunVersion": 2,
  "executionRoleArn": "arn:aws:iam::378118473695:role/aws-elasticbeanstalk-service-role",
  "containerDefinitions": [
    {
      "name": "service-auth",
      "image": "agranom/service-auth",
      "memory": 128,
      "portMappings": [
        {
          "containerPort": "3001"
        }
      ],
      "secrets": [
        {
          "name": "DB_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:378118473695:secret:todo-app-be-credentials-B8yY1X/SVC_AUTH_DB_URL"
        },
        {
          "name": "INTERNAL_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:378118473695:secret:todo-app-be-credentials-B8yY1X/INTERNAL_PASSWORD"
        }
      ],
      "environment": [
        {
          "name": "INTERNAL_USER",
          "value": "agranom"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "PORT",
          "value": 3001
        }
      ]
    }
  ]
}
